package feng3d.events
{
	import flash.events.Event;

	/**
	 *
	 * @author warden_feng 2014-3-19
	 */
	public class FengEvent extends Event
	{
		public var data:*;

		public function FengEvent(type:String, data:* = null, bubbles:Boolean = false, cancelable:Boolean = false)
		{
			super(type, bubbles, cancelable);
			this.data = data;
		}
	}
}
